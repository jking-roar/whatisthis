<project>
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.kingjoshdavid</groupId>
    <artifactId>javafun</artifactId>
    <packaging>pom</packaging>
    <version>none</version>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.3</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <modules>
        <module>plain-jain</module>
        <module>spring-boot</module>
        <module>hibernate</module>
        <module>look-intellij</module>
        <module>music</module>
    </modules>
</project>
